#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>

using namespace std;
int main() {
    int t,cnt1=0,cnt=0;
    cin>>t;
    int A[t];
    for(int i=0;i<t;i++)
        {
        cin>>A[i];
        cnt=cnt+A[i];
        cnt1=max(cnt,cnt1);
        if(cnt<0)
            cnt=0;
    }
    cout<<cnt1;
    return 0;
}
